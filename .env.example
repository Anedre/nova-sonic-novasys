# .env.example
# ⚠️ IMPORTANTE: Copia este archivo a .env y configura tus credenciales reales
# Nunca subas el archivo .env al repositorio

# AWS Credentials (obtener de IAM Console)
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_HERE
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY_HERE
AWS_REGION=us-east-1

# Si usas credenciales temporales (STS), descomenta:
# AWS_SESSION_TOKEN=YOUR_SESSION_TOKEN_HERE

# Opciones avanzadas (opcional)
# NOVA_SONIC_DEBUG=false
# NOVA_SONIC_DIAGNOSTICS=false
# NOVA_SONIC_SILENCE_TIMEOUT_DEFAULT=0.8
# NOVA_SONIC_SILENCE_TIMEOUT_FAST=0.5
# NOVA_SONIC_WEBM_INIT_BYTES=16384
# NOVA_SONIC_WEBM_TIMEOUT_S=2.0
